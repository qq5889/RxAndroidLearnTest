<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.skyinno.rxandroidlearntest"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.SimpleRxActivity">
        </activity>
        <activity android:name=".activity.MapRxActivity">
        </activity>
        <activity android:name=".activity.FilterActivity">
        </activity>
        <activity android:name=".activity.CombineActivity">
        </activity>
        <activity android:name=".activity.CatchActivity">
        </activity>
        <activity android:name=".activity.FunctionActivity">
        </activity>
        <activity android:name=".activity.FactorActivity">
        </activity>
        <activity android:name=".activity.CalculateActivity">
        </activity>
        <activity android:name=".activity.ConnectActivity">
        </activity>
    </application>

</manifest>