<?xml version="1.0" encoding="utf-8"?>
<ScrollView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <Button
            android:id="@+id/btn_debounce"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".debounce过滤基础用法"/>

        <Button
            android:id="@+id/btn_distinct"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".distinct过滤基础用法"/>

        <Button
            android:id="@+id/btn_distinctUntilChanged"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".distinctUntilChanged过滤基础用法"/>

        <Button
            android:id="@+id/btn_elementAt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".elementAt过滤基础用法"/>

        <Button
            android:id="@+id/btn_filter"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".filter过滤基础用法"/>

        <Button
            android:id="@+id/btn_ofType"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text=".ofType过滤基础用法"/>
    </LinearLayout>
</ScrollView>
